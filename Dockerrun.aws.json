{
  "AWSEBDockerrunVersion": "1",
  "image": "eclipse-temurin:17-jre-alpine",
  "command": "java -jar app.jar",
  "portMappings": [
    {
      "hostPort": 8080,
      "containerPort": 8080
    }
  ],
  "volumes": [
    {
      "hostDirectory": "/var/app/current/uploads",
      "containerDirectory": "/var/app/current/uploads"
    }
  ]
} 